<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<fragment
   id="org.holongate.j2d.win32"
   name="%fragment.name"
   version="2.3.0"
   provider-name="Holongate.org"
   plugin-id="org.holongate.j2d"
   plugin-version="2.3.0"
   match="greaterOrEqual">

   <runtime>
      <library name="j2d4swt-win32.jar">
         <packages prefixes="org.holongate.j2d.win32"/>
         <export name="*"/>
      </library>
   </runtime>


   <extension
         name="Graphics2D Factories"
         point="org.holongate.j2d.factories">
      <factory
            platform="win32.win32.x86"
            name="%win32.factory"
            class="org.holongate.j2d.win32.Win32Graphics2DFactory">
      </factory>
   </extension>
</fragment>
